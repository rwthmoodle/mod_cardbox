<div id="cardbox-practice" class="container-fluid cardbox-practice">

    <div id="cardbox-practice-replacable">
        {{#case1}}
            {{> mod_cardbox/practice_question_selfcheck }}
        {{/case1}}

        {{#case2}}
            {{> mod_cardbox/practice_question_autocheck }}
        {{/case2}}

        {{#case3}}
            {{> mod_cardbox/practice_answer_selfcheck }}
        {{/case3}}

        {{#case4}}
            {{> mod_cardbox/practice_answer_autocheck }}
        {{/case4}}

        {{#case5}}
            {{> mod_cardbox/practice_suggest_answer }}
        {{/case5}}
        
    </div>

